{
    "manifest_version": 3,
    "name": "Tabs Saver",
    "version": "1.0",
    "description": "Save and manage your tabs.",
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon-16.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png"
        }
    },
    "icons": {
        "16": "images/icon-16.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}
